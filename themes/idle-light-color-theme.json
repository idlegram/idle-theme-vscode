  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#408061",
    "property": "#408080",
    "interface": "#408061",
    "type": "#408061",
    "class": "#408061"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "scope": [
        "delimiter.bracket",
        "delimiter",
        "invalid.illegal.character-not-allowed-here.html",
        "keyword.operator.rest",
        "keyword.operator.spread",
        "keyword.operator.type.annotation",
        "keyword.operator.relational",
        "keyword.operator.assignment",
        "keyword.operator.type",
        "meta.brace",
        "meta.tag.block.any.html",
        "meta.tag.inline.any.html",
        "meta.tag.structure.input.void.html",
        "meta.type.annotation",
        "meta.embedded.block.github-actions-expression",
        "storage.type.function.arrow",
        "meta.objectliteral.ts",
        "punctuation",
        "punctuation.definition.string.begin.html.vue",
        "punctuation.definition.string.end.html.vue"
      ],
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.language",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#a69553"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name"
      ],
      "settings": {
        "foreground": "#4d8199"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#35393b"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "tag.html"
      ],
      "settings": {
        "foreground": "#408080"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#4d8199"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type.class.jsdoc",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#408080"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type",
        "support.type.builtin",
        "constant.language.undefined",
        "constant.language.null",
        "constant.language.import-export-all.ts"
      ],
      "settings": {
        "foreground": "#a65363"
      }
    },
    {
      "scope": [
        "text.html.derivative",
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#35393b"
      }
    },
    {
      "scope": [
        "string",
        "string punctuation.section.embedded source",
        "attribute.value"
      ],
      "settings": {
        "foreground": "#a67453"
      }
    },
    {
      "scope": [
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#a6745377"
      }
    },
    {
      "scope": [
        "punctuation.support.type.property-name"
      ],
      "settings": {
        "foreground": "#40808077"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#408080"
      }
    },
    {
      "scope": [
        "property",
        "meta.property-name",
        "meta.object-literal.key",
        "entity.name.tag.yaml",
        "attribute.name"
      ],
      "settings": {
        "foreground": "#408080"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "invalid.deprecated.entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#a69553"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#a69553"
      }
    },
    {
      "scope": [
        "support.type.primitive",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#408061"
      }
    },
    {
      "scope": "namespace",
      "settings": {
        "foreground": "#408061"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment.compound",
        "meta.var.expr.ts"
      ],
      "settings": {
        "foreground": "#a65363"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b91c1c"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b91c1c"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b91c1c"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b91c1c"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#fafafa"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#b91c1c"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#a67453"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#a65363"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repetition"
      ],
      "settings": {
        "foreground": "#a67453"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "foreground": "#a69553"
      }
    },
    {
      "scope": [
        "support.constant"
      ],
      "settings": {
        "foreground": "#a69553"
      }
    },
    {
      "scope": [
        "keyword.operator.quantifier.regexp",
        "constant.numeric",
        "number"
      ],
      "settings": {
        "foreground": "#a65385"
      }
    },
    {
      "scope": [
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#a65363"
      }
    },
    {
      "scope": [
        "constant.language.boolean",
        "constant.language"
      ],
      "settings": {
        "foreground": "#a65385"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#408080"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#a67453"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#408080"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#408061"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#35393b"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#35393b"
      }
    },
    {
      "scope": "markup.raw",
      "settings": {
        "foreground": "#408080"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "foreground": "#b91c1c"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "foreground": "#16a34a"
      }
    },
    {
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "foreground": "#ea580c"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#f5f5f5"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#a855f7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#2563eb"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2563eb"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#2563eb"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#525252"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#b91c1c"
      }
    },
    {
      "scope": [
        "constant.other.reference.link",
        "string.other.link",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown"
      ],
      "settings": {
        "foreground": "#a67453"
      }
    },
    {
      "scope": [
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#35393b90",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "type.identifier",
        "constant.other.character-class.regexp"
      ],
      "settings": {
        "foreground": "#408061"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.html.vue"
      ],
      "settings": {
        "foreground": "#4d8199"
      }
    },
    {
      "scope": [
        "invalid.illegal.unrecognized-tag.html"
      ],
      "settings": {
        "fontStyle": ""
      }
    }
  ]
}
